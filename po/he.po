#: src/DeviceRow.vala:46
msgid "Connecting…"
msgstr "מתבצעת התחברות…"

#: src/DeviceRow.vala:48
msgid "Disconnecting…"
msgstr "מתבצע ניתוק…"

#: src/DeviceRow.vala:50
msgid "Unable to Connect"
msgstr "לא ניתן להתחבר"

#: src/DeviceRow.vala:52
msgid "Not Connected"
msgstr "אין חיבור"

