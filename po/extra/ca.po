#: data/bluetooth.metainfo.xml.in:94
msgid "Show \"Discovering\" indicator in an overlay bar"
msgstr "Es mostra l'indicador \"Descobrint\" en una barra de superposició."

#: data/bluetooth.metainfo.xml.in:102
msgid "Fixes"
msgstr "Correccions"

#: data/bluetooth.metainfo.xml.in:104
msgid "Fix crash on devices without Bluetooth"
msgstr "Correcció de la fallada en dispositius sense Bluetooth"

#: data/bluetooth.metainfo.xml.in:108
